<h1>Listing posted_shifts</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Date</th>
      <th>Start time</th>
      <th>Finish time</th>
      <th>Seller</th>
      <th>Buyer</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posted_shifts.each do |posted_shift| %>
      <tr>
        <td><%= posted_shift.position %></td>
        <td><%= posted_shift.date %></td>
        <td><%= posted_shift.start_time %></td>
        <td><%= posted_shift.finish_time %></td>
        <td><%= posted_shift.seller_id %></td>
        <td><%= posted_shift.buyer_id %></td>
        <td><%= posted_shift.status %></td>
        <td><%= link_to 'Show', posted_shift %></td>
        <td><%= link_to 'Edit', edit_posted_shift_path(posted_shift) %></td>
        <td><%= link_to 'Destroy', posted_shift, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posted shift', new_posted_shift_path %>
